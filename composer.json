{
    "name": "kristuff/minikit-server",
    "description": "Mini php web framework",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Kristuff",
            "homepage": "https://github.com/kristuff"
        }
    ],
    "require": {
        "php": ">=7.3",
        "kristuff/patabase": "^1.0-stable",
        "kristuff/mishell": "^1.5-stable",
        "kristuff/phtemail": "^0.2-stable",
        "phpmailer/phpmailer": "^6.1-stable"
    },
     "autoload": {
        "psr-4": {"Kristuff\\Minikit\\": "lib/"}
    },
    "autoload-dev": {
        "psr-4": {
            "Kristuff\\Minikit\\Tests\\": "tests/",
            "MyAwesomeApp\\": "tests/_data/mvc_app_namespace/controller/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5 || ^8"
    }
}
